{
  "name": "@tanker/client-react-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace ClientReactNativeExample.xcworkspace -scheme ClientReactNativeExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-base64": "0.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-launch-arguments": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/metro-config": "^0.73.5",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/jest": "^28.1.2",
    "@types/react-native-base64": "^0.2.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.19.0",
    "jest": "^29.6.3",
    "pod-install": "^0.1.0",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=16"
  }
}
